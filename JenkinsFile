@Library('myPipeline') _ 

pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                bat 'Pythonzip.py'
            }
        }
        stage ('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy'){
            steps {
                bat 'sharelibjf-zip.py'
            }
        }
    }
    post {
    always {
      sendNotification currentBuild.result
    }
  }
}
