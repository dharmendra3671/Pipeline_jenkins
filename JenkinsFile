pipeline {
    agent any
    environment {
        branch = 'main'
        scmUrl = 'https://github.com/dharmendra3671/Pipeline_jenkins.git'
        serverPort = '8080'
        }
        stages {
            stage('checkout git') {
                steps {
                    git branch: branch, url: scmUrl
                }
            }

            stage('build') {
                steps {
                    bat 'Pythonzip.py'
                }
            }

            stage ('test') {
                steps {
                    echo 'testing'
                }
            }

            stage('deploy'){
                steps {
                    bat 'sharelibjf-zip.py'
                }
            }
        }
    }

