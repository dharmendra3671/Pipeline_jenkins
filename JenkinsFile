@Library('mypipeline_notification') _

pipeline {
    agent any
    stages {
        stage('build') {
            steps {
               bat "${get_resource_dir()}/Scripts/Pythonzip.py"
               bat "${get_resource_dir()}/Scripts/config.json"
            }
        }
        stage ('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy'){
            steps {
                bat "${get_resource_dir()}/Scripts/sharelibjf-zip.py"
            }
        }
    }
    post {
        always {
            sendNotification currentBuild.result
        }
    }
}
