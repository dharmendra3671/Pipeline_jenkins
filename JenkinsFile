@Library('mypipeline_notification') _

pipeline {
    agent any
    stages {
        stage('build') {
            steps {
               libraryResource("Pythonzip.py")
            }
        }
        stage ('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy'){
            steps {
                libraryResource("sharelibjf-zip.py")
            }
        }
    }
    post {
        always {
            sendNotification currentBuild.result
        }
    }
}
