@Library('mypipeline_notification') _

pipeline {
    agent any
    stages {
        stage('build') {
            steps {
               bat "${get_resource_dir()}/"
            }
        }
        stage ('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy'){
            steps {
                sh "${get_resource_dir()}/com/example/test.sh"
            }
        }
    }
}
