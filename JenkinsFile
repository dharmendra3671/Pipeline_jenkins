@Library('mypipeline_notification') _

pipeline {
    agent any
    stages {
        stage('build') {
            steps {
               libraryResource 'resources/Scripts/Pythonzip.py'
            }
        }
        stage ('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy'){
            steps {
                libraryResource 'resources/Scripts/sharelibjf-zip.py'
            }
        }
    }
    post {
        always {
            sendNotification currentBuild.result
        }
    }
}
